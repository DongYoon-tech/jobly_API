{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "The root schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "examples": [
        {
            "username": "User123",
            "password": "password1234",
            "first_name": "Joe",
            "last_name": "Smith",
            "email": "joesmith@gmail.com",
            "photo_url": "https://cdn.iconscout.com/icon/free/png-512/laptop-user-1-1179329.png",
            "is_admin": false
        }
    ],
    "required": [
        "username",
        "password",
        "first_name",
        "last_name",
        "email"
    ],
    "properties": {
        "username": {
            "$id": "#/properties/username",
            "type": "string",
            "title": "The username schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "User123"
            ]
        },
        "password": {
            "$id": "#/properties/password",
            "type": "string",
            "title": "The password schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "password1234"
            ]
        },
        "first_name": {
            "$id": "#/properties/first_name",
            "type": "string",
            "title": "The first_name schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "Joe"
            ]
        },
        "last_name": {
            "$id": "#/properties/last_name",
            "type": "string",
            "title": "The last_name schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "Smith"
            ]
        },
        "email": {
            "$id": "#/properties/email",
            "type": "string",
            "title": "The email schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "joesmith@gmail.com"
            ]
        },
        "photo_url": {
            "$id": "#/properties/photo_url",
            "type": "string",
            "title": "The photo_url schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "https://cdn.iconscout.com/icon/free/png-512/laptop-user-1-1179329.png"
            ]
        },
        "is_admin": {
            "$id": "#/properties/is_admin",
            "type": "boolean",
            "title": "The is_admin schema",
            "description": "An explanation about the purpose of this instance.",
            "default": false,
            "examples": [
                false
            ]
        }
    },
    "additionalProperties": true
}